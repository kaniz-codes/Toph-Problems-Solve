#include <iostream>
#include <string>
using namespace std;
int main()
{
    int A,n, count=0,i;
    cin>> A;
    string num = to_string(A);
    n= num.size();
    count =n%3;
    for(i=0;i<n; ++i)
    { cout<< num[i];
    count--;
      if (count ==0 && i !=n-1)
       {
        cout<< ',';
        count = 3;
       }
    }
    cout<<endl;
    return 0;
}
